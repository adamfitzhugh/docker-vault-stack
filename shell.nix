# Add additional packages that are needed for this specific repo that aren't included in your HM config
{ pkgs ? import <nixpkgs> {} }:

let
  # Install specific Terraform package from nixos-config
  terraform = pkgs.callPackage ../../nixos-config/pkgs/terraform/default-arm64.nix {};
  vault = pkgs.callPackage ../../nixos-config/pkgs/vault/default-arm64.nix {}; # Use the specific version of Vault you need
in

pkgs.mkShell {
  buildInputs = [ 
    terraform
    vault
    pkgs.jq
  ];


# Add extra environment variables here.
EXAMPLE = "Example Env Var";
VAULT_ADDR = "http://0.0.0.0:9200";
VAULT_LICENSE = "";

}